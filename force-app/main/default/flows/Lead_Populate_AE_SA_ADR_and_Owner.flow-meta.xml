    <start>
        <locationX>391</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Is_Lead_Source_equal_to_MSP_Registered</targetReference>
        </connector>
        <filterFormula>AND(
  OR(
    ISNEW(),
    ISCHANGED({!$Record.PostalCode}),
    ISCHANGED({!$Record.LeadSource}),
    ISCHANGED({!$Record.Country}),
    ISCHANGED({!$Record.State})
  ),
  NOT(
    OR(
      ISPICKVAL({!$Record.Status}, &apos;Qualified&apos;),
      ISPICKVAL({!$Record.Status}, &apos;Disqualified&apos;)
    )
  )
)</filterFormula>
        <object>Lead</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
