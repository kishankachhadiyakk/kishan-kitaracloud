    <formulas>
        <name>varCompanyFirstLetter</name>
        <dataType>String</dataType>
        <expression>IF( ( {!$Record.State} = &apos;NY&apos; || {!$Record.State} = &apos;NJ&apos; ), IF( ISBLANK( {!$Record.Company} ) , null, LEFT( {!$Record.Company} , 1 )) , null )</expression>
    </formulas>
    <formulas>
        <name>varState</name>
        <dataType>String</dataType>
        <expression>IF( ISBLANK( {!$Record.State} ), &apos;NULL&apos;, ( IF ( {!$Record.Country} &lt;&gt; &apos;United States&apos; , &apos;NULL&apos; , {!$Record.State} ) ) )</expression>
    </formulas>
    <formulas>
        <name>varZipCodeSplit</name>
        <dataType>String</dataType>
        <expression>IF( TEXT( {!$Record.Zip_Code_Split__c} ) = &apos;&apos; ,  &apos;NULL&apos;, TEXT( {!$Record.Zip_Code_Split__c}))</expression>
    </formulas>
